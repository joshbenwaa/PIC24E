#define CS    PIN_B6
#define TOGGLE PIN_A0
#pin_select U1TX=PIN_B11
#pin_select U1RX=PIN_B12
#use rs232(UART1, baud=115200, stream=UART_PORT1)

#use spi(FORCE_HW, MASTER, SPI1, MODE=1, stream=SPI_PORT1, BAUD = 12000000)
