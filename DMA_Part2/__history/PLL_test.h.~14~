#include <24EP256GP202.h>
#device ICSP=1

#FUSES HS, NOWDT, PR_PLL        //No Watch Dog Timer
#FUSES NOJTAG                   //JTAG disabled
#FUSES CKSFSM                   //Clock Switching is enabled, fail Safe clock monitor is enabled
#define LED PIN_A0
#define DELAY 1
#use delay(clock=140MHz,crystal = 10MHz, PLL_WAIT)
